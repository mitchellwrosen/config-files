# Increase volume
"amixer set Master playback 1+"
   m:0x0 + c:123
   XF86AudioRaiseVolume

# Decrease volume
"amixer set Master playback 1-"
   m:0x0 + c:122
   XF86AudioLowerVolume

# Increase brightness
"xbacklight -inc 10 -time 0"
   XF86MonBrightnessUp

# Decrease brightness
"xbacklight -dec 10 -time 0"
   XF86MonBrightnessDown
